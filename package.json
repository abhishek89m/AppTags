{
  "name": "app_tagit",
  "version": "1.0.0",
  "description": "Simple tags management app demo. Add, edit or delete tags.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm install && gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishek89m/AppTags.git"
  },
  "keywords": [
    "Tags",
    "KnockoutJS",
    "Gulp",
    "LESS",
    "Bootstrap"
  ],
  "author": "Abhishek Mukherjee",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/abhishek89m/AppTags/issues"
  },
  "homepage": "https://github.com/abhishek89m/AppTags#readme",
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-global-shim"
    ]
  },
  "browserify-global-shim": {
    "jQuery": "$",
    "knockout": "ko"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "browserify-global-shim": "^1.0.3",
    "browserify-shim": "^3.8.14",
    "express": "^4.16.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^4.0.0",
    "gulp-minify-css": "^1.2.4",
    "stream-combiner2": "^1.1.1",
    "stringify": "^5.2.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "jquery": "^1.12.4",
    "bootstrap": "^3.3.7"
  }
}
